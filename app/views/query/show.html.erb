<h1>
  <%= @solutions.count %>
  <%= @sparql_file %>

</h1>

<div class="table-responsive">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">#</th>
        <% @solutions.variable_names.each do |head| %>
          <th scope="col"><%= head.to_s %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% @solutions.each.with_index do | solution, index| %>
        <tr>
          <th scope="row"><%= index + 1 %></th>
          <% @solutions.variable_names.each do |key| %>
            <td>
              <%= display_object(solution[key]) if solution[key] %>
            </td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
