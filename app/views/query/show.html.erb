<h1>
  <%= @solutions.count %>
  Results
</h1>

<div class="table-responsive">
  <table class="table  table-hover">
    <thead>
      <tr>
        <th scope="col">#</th>
        <% @solutions.first.each do |head| %>
          <th scope="col"><%= head[0] %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% @solutions.each.with_index do | solution, index| %>
        <tr>
          <th scope="row"><%= index + 1 %></th>
          <% solution.each do |statement| %>
            <td>
              <%= display_object(statement[1]) %>
            </td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
