PREFIX schema: <http://schema.org/>
PREFIX prov: <http://www.w3.org/ns/prov#>
select ?entities (count(?e) as ?Organizations)  ?dataset ?graph  ?generatedAtTime  where 
{
    ?graph a schema:Dataset .
    filter(!STRSTARTS(str(?graph),"http://kg.artsdata.ca/minted/K"))
    graph ?graph {
         ?e a ?type .
    }
    filter exists {
        ?e a schema:Organization  .
    } 
    filter(!isBLANK(?e))
    ?graph rdfs:label ?dataset .
    OPTIONAL {
        ?graph prov:generatedAtTime ?generatedAtTime .
    }
    
    bind(ENCODE_FOR_URI(str(?dataset)) as ?dataset_escaped)
    bind(CONCAT("<a href='/query/show?title=Organization entities in ",str(?dataset_escaped),"&sparql=list_organizations&graph=",str(?graph),"'>View</a>") as ?entities)
 
} group by ?graph ?dataset ?generatedAtTime ?entities order by DESC(?generatedAtTime)